{
  "compilerOptions": {
    "target": "es6",
    "useDefineForClassFields": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist/server",
    "declaration": true,
  },
  "include": ["src/server"],
  "lib": ["es2015"]
}
